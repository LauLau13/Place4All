{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false, //no requiere autentificaci贸n para entrar
    "anonymousAuthentication": true, //acceso an贸nimo
    "iisExpress": {
      "applicationUrl": "http://localhost:34964", //localhost de lanzamiento del front de la aplicaci贸n
      "sslPort": 44352
    }
  },
  //para ejecutar e back de la api mediante el swagger
  "profiles": {
    "WebApi": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7157;http://localhost:5157", //localhost para acceder al swagger
